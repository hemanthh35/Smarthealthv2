        const translations = {
            en: { title: "Contact Nearby Hospitals", content: "This is a multilingual webpage." },
            es: { title: "Contactar Hospitales Cercanos", content: "Esta es una página web multilingüe." },
            fr: { title: "Contacter les Hôpitaux à Proximité", content: "Ceci est une page Web multilingue." },
